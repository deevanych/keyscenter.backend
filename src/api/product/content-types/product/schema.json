{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {}
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "price": {
      "type": "integer",
      "default": 0,
      "required": false,
      "min": 0,
      "pluginOptions": {}
    },
    "salePrice": {
      "type": "integer",
      "min": 0,
      "default": 0,
      "pluginOptions": {}
    },
    "product_keys": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-key.product-key",
      "mappedBy": "product"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "slug": {
      "type": "uid",
      "unique": true,
      "targetField": "title"
    },
    "platforms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::platform.platform",
      "mappedBy": "product"
    },
    "product_type": {
      "type": "enumeration",
      "enum": [
        "Цифровой ключ",
        "Логин/пароль",
        "Ссылка"
      ],
      "default": "Цифровой ключ",
      "pluginOptions": {}
    },
    "product_delivery": {
      "type": "enumeration",
      "enum": [
        "Ручная",
        "Автоматическая"
      ],
      "default": "Автоматическая",
      "pluginOptions": {}
    },
    "views": {
      "type": "biginteger",
      "default": "0",
      "min": "0",
      "pluginOptions": {}
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "instruction": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
